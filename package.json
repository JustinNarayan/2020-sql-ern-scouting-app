{
   "name": "2020-mern-scouting-app",
   "version": "1.0.0",
   "description": "Refactored 2020 Alt-F4 Scouting App using the MERN Stack",
   "main": "server.js",
   "scripts": {
      "client-install": "npm install --prefix client",
      "start": "node server.js",
      "server": "nodemon server.js",
      "client": "npm start --prefix client",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/jnarayanbvg/2020-mern-scouting-app.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/jnarayanbvg/2020-mern-scouting-app/issues"
   },
   "homepage": "https://github.com/jnarayanbvg/2020-mern-scouting-app#readme",
   "dependencies": {
      "bcrypt": "^5.0.0",
      "body-parser": "^1.19.0",
      "concurrently": "^5.2.0",
      "cors": "^2.8.5",
      "email-validator": "^2.0.4",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.18.1",
      "nodemailer": "^6.4.10",
      "nodemailer-smtp-transport": "^2.7.4",
      "react-router-dom": "^5.2.0",
      "uuid": "^8.2.0"
   },
   "devDependencies": {
      "nodemon": "^2.0.4"
   }
}
